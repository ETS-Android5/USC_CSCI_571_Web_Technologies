import { isDevMode } from '@angular/core';

export class AppConfig {
  onLocal = true;

  constructor() {
    this.onLocal = isDevMode();
  }

  getApiEndPoint() {
    if (this.onLocal) {
      return '<Development URL>';
    } else {
      return '<Production URL>';
    }
  }

  getFacebookAppId() {
    return '';
  }
}
